    "@react-navigation/native-stack": "^6.9.8",
    "@reduxjs/toolkit": "^1.9.1",
    "@tanstack/react-query": "^4.22.0",
    "date-fns": "^2.29.3",
    "react-hook-form": "^7.41.5",
    "react": "18.2.0",
    "react-native": "0.71.7",
    "react-native-gesture-handler": "^2.8.0",
    "react-native-linear-gradient": "^2.6.2",
    "react-native-mmkv": "^2.5.1",
